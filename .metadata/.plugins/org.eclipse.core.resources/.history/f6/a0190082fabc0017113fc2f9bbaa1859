package model;

public class Product 
{
	String pictureAddress;
	String productName;
	String productPrice;
	String productStock;
	String productExplan;
	String productCode;
	
	//기본 생성자
	public Product(){}
	
	public Product(String pictureAddress, String productName, String productPrice, String productStock , String productExplan, String productCode)
	{
		this.pictureAddress = pictureAddress;
		this.productName = productName;
		this.productPrice = productPrice;
		this.productStock = productStock;
		this.productExplan = productExplan;
		this.productCode = productCode;
	} // end of constructor

} // end of Item
