package model;

public class User 
{
    public int studentNumber;
    public int password;
    public String name;
    public int pinCode;

    public User(int studentNumber, int password, String name, int pinCode) 
    {
    	  this.studentNumber = studentNumber;
          this.password = password;
          this.name = name;
          this.pinCode = pinCode;
    } // end of cosntructor
    
} // end of User
